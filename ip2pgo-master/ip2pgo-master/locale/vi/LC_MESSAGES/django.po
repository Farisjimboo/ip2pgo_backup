msgstr "Người mua"

#: ip2pwebsite/templates/termofuse.html:89
#: ip2pwebsite/templates/termofuse.html:102
#: ip2pwebsite/templates/termofuse.html:108
msgid "Seller"
msgstr "người bán"

#: ip2pwebsite/templates/termofuse.html:90
msgid "Dispute initiator"
msgstr "Người khởi xướng tranh chấp"

#: ip2pwebsite/templates/termofuse.html:93
msgid "1"
msgstr "1"

#: ip2pwebsite/templates/termofuse.html:94
msgid "Buyer has paid in full"
msgstr "Người mua đã thanh toán đầy đủ"

#: ip2pwebsite/templates/termofuse.html:95
msgid "Seller has received in full, but did not release the escrow"
msgstr "Người bán đã nhận được đầy đủ, nhưng không phát hành ký quỹ"

#: ip2pwebsite/templates/termofuse.html:96
msgid "Buyer or automatically by the system if the countdown ended"
msgstr ""
"Người mua hoặc tự động bởi hệ thống khi việc đếm ngược thời gian đã kết thúc"

#: ip2pwebsite/templates/termofuse.html:99
msgid "2"
msgstr ""

